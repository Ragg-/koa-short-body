{
  "name": "koa-short-body",
  "description": "More short API for koa-better-body.",
  "version": "1.1.0",
  "scripts": {
    "lint": "jshint index.js && jscs index.js --reporter inline --esnext",
    "test": "mocha --require should",
    "test-cov": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --require should",
    "test-travis": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --require should",
    "examples-multer": "npm run test && node --harmony examples/multer.js",
    "examples-koa-router": "npm run test && node --harmony examples/koa-router.js"
  },
  "authors": [
    {
      "name": "Charlike Make Reagent",
      "email": "mameto_100@mail.bg",
      "url": "https://github.com/tunnckoCore"
    },
    {
        "name": "Ragg",
        "url": "https://github.com/Ragg-"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Ragg-/koa-short-body.git"
  },
  "keywords": [
    "api",
    "body",
    "bodyparser",
    "csp",
    "csp-report",
    "data",
    "form",
    "formdata",
    "formidable",
    "json",
    "koa",
    "middleware",
    "multipart",
    "multiple",
    "parse",
    "parser",
    "report",
    "request",
    "upload",
    "urlencoded"
  ],
  "license": {
    "type": "MIT",
    "url": "https://github.com/Ragg-/koa-short-body/blob/master/license.md"
  },
  "dependencies": {
    "arr-union": "~3.0.0",
    "co-better-body": "^1.0.1",
    "extend": "~3.0.0",
    "formidable": "~1.0.17"
  },
  "devDependencies": {
    "koa": "*",
    "koa-router": "*",
    "koa-resource-router": "*",
    "multiline": "*",
    "should": "*",
    "supertest": "*",
    "istanbul-harmony": "~0.3.16",
    "mocha": "*"
  },
  "engines": {
    "node": ">=0.11.14",
    "npm": ">=1.4.28"
  }
}
